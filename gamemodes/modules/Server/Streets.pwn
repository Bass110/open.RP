stock GetPlayerStreet(playerid)
{
	new Float:x,
	    Float:y,
		Float:z,
		RealStreetCode;

	GetPlayerPos(playerid, x, y, z);

	new zonename[64];

 	for(new zone = 0; zone < 365; ++zone)
 	{
		if(x >= StreetInfo[zone][zMinx] && x <= StreetInfo[zone][zMaxx] && y >= StreetInfo[zone][zMiny] && y <= StreetInfo[zone][zMaxy])
		{
			RealStreetCode = floatround(StreetInfo[zone][zMinx]);
			if(RealStreetCode < 0) RealStreetCode = RealStreetCode*(-1);
		    format(zonename, sizeof(zonename), "%d %s", RealStreetCode, StreetInfo[zone][zName]);
		    break;
		}
	}
	return zonename;
}

stock GetVehicleStreet(vehicleid)
{
	new Float:x,
	    Float:y,
		Float:z,
		RealStreetCode;

	GetVehiclePos(vehicleid, x, y, z);

	new zonename[64];

 	for(new zone = 0; zone != 366; ++zone)
 	{
		if(x >= StreetInfo[zone][zMinx] && x <= StreetInfo[zone][zMaxx] && y >= StreetInfo[zone][zMiny] && y <= StreetInfo[zone][zMaxy])
		{
			RealStreetCode = floatround(StreetInfo[zone][zMinx]);
			if(RealStreetCode < 0) RealStreetCode = RealStreetCode*(-1);
		    format(zonename, sizeof(zonename), "%d %s", RealStreetCode, StreetInfo[zone][zName]);
		    break;
		}
	}
	return zonename;
}

stock GetStreetNamePos(Float:snPosX, Float:snPosY, Float:snPosZ)
{
	new zonename[64],
		RealStreetCode;
		
 	for(new zone = 0; zone != 366; ++zone)
 	{
		if(snPosX >= StreetInfo[zone][zMinx] && snPosX <= StreetInfo[zone][zMaxx] && snPosY >= StreetInfo[zone][zMiny] && snPosY <= StreetInfo[zone][zMaxy])
		{
			RealStreetCode = floatround(StreetInfo[zone][zMinx]);
			if(RealStreetCode < 0) RealStreetCode = RealStreetCode*(-1);
		    format(zonename, sizeof(zonename), "%d %s, SA Sky, Height of %d n/m", RealStreetCode, StreetInfo[zone][zName], floatround(snPosZ));
		    break;
		}
	}
	return zonename;
}

